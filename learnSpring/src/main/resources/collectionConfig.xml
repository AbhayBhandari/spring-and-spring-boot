<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
       http://www.springframework.org/schema/context
       http://www.springframework.org/schema/context/spring-context.xsd">

    <bean class="org.example.setterInjection.collections.Employee" name="Employee1">
        <property name="employeeName">
            <value>Abhay Bhandari</value>
        </property>

        <property name="mobileNumber">
            <list>
                <value>871</value>
                <value>455</value>
                <value>785</value>
                <value>78951</value>
            </list>
        </property>

        <property name="address">
            <set>
                <value>india</value>
                <value>pune</value>
                <value>kharadi</value>
                <value>mumbai</value>
            </set>
        </property>

        <property name="courses">
            <map>
                <entry key="java" value="2023"/>
                <entry key="c++" value="2018"/>
                <entry key="javascript" value="2021"/>
                <entry key="python" value="2019"/>
            </map>
        </property>
    </bean>
</beans>